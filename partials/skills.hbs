{{#if resume.skills.length}}
\subsectioncustom{Skills Summary}

\begin{itemize*}
  %\renewcommand\labelitemi{$\rightarrow$}
	{{#each resume.skills}}
        \item{
            {{name}} :
		    {{#if keywords.length}}
		    {{#each keywords}}
                {{#if @index}}, {{/if}} {{.}}
		    {{/each}}
		    {{/if}}
        }
	{{/each}}
  %\item{ {{resume.basics.summary}} }
\end{itemize*}
{{/if}}

